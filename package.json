{
  "name": "xcfreader",
  "version": "0.0.6",
  "description": "Reads in GIMP xcf files",
  "main": "lib/gimpparser.js",
  "dependencies": {
    "binary-parser": "^1.1.5",
    "lazy.js": "^0.4.2",
    "pngjs-image": "^0.11.6"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "jsonfile": "^2.3.1",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.9.2"
  },
  "scripts": {
    "multi": "nodemon --exec babel-node examples/multi.js",
    "single": "nodemon --exec babel-node examples/single.js",
    "map": "nodemon --exec babel-node examples/map.js",
    "text": "nodemon --exec babel-node examples/text.js",
    "compile": "babel -d lib src/",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/crazyapesoftware/xcfreader.git"
  },
  "keywords": [
    "gimp",
    "xcf",
    "parser"
  ],
  "author": "Andi McLean",
  "license": "MIT",
  "homepage": "https://bitbucket.org/crazyapesoftware/xcfreader#readme"
}
