{
  "name": "xcfreader",
  "version": "0.0.8",
  "description": "Reads in GIMP xcf files",
  "type": "module",
  "main": "dist/gimpparser.js",
  "types": "dist/gimpparser.d.ts",
  "dependencies": {
    "binary-parser": "^1.1.5",
    "pngjs-image": "^0.11.6"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "nodemon": "^2.0.21",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "multi": "npm run build && nodemon --exec node dist/examples/multi.js",
    "single": "npm run build && nodemon --exec node dist/examples/single.js",
    "map": "npm run build && nodemon --exec node dist/examples/map.js",
    "text": "npm run build && nodemon --exec node dist/examples/text.js",
    "test": "npm run build && node dist/tests/runner.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:andimclean/xcfreader.git"
  },
  "keywords": [
    "gimp",
    "xcf",
    "parser"
  ],
  "author": "Andi McLean",
  "license": "MIT",
  "homepage": "https://github.com/andimclean/xcfreader#readme"
}
