{
  "name": "xcfreader",
  "version": "0.0.7",
  "description": "Reads in GIMP xcf files",
  "type": "module",
  "main": "src/gimpparser.js",
  "dependencies": {
    "binary-parser": "^1.1.5",
    "lazy.js": "^0.4.2",
    "pngjs-image": "^0.11.6"
  },
  "devDependencies": {
    "jsonfile": "^2.3.1",
    "mkdirp": "^0.5.1",
    "nodemon": "^2.0.21"
  },
  "scripts": {
    "multi": "nodemon --exec node --experimental-specifier-resolution=node examples/multi.js",
    "single": "nodemon --exec node --experimental-specifier-resolution=node examples/single.js",
    "map": "nodemon --exec node --experimental-specifier-resolution=node examples/map.js",
    "text": "nodemon --exec node --experimental-specifier-resolution=node examples/text.js",
    "compile": "echo \"No compile step: project uses native ESM; edit source in src/ and run examples with node\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:andimclean/xcfreader.git"
  },
  "keywords": [
    "gimp",
    "xcf",
    "parser"
  ],
  "author": "Andi McLean",
  "license": "MIT",
  "homepage": "https://github.com/andimclean/xcfreader#readme"
}
